{
    "publisher": "agroszer",
    "name": "manage-all",
    "displayName": "All Manager",
    "description": "All Manager",
    "version": "1.5.0",
    "icon": "media/clipboard.png",
    "extensionKind": [
        "ui"
    ],
    "engines": {
        "vscode": "^1.65.0"
    },
    "bugs": {
        "url": "https://github.com/agroszer/vscode.manage-all/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/agroszer/vscode.manage-all.git"
    },
    "homepage": "https://github.com/agroszer/vscode.manage-all",
    "categories": [
        "Other"
    ],
    "keywords": [
        "clipboard",
        "copy",
        "ditto",
        "history",
        "paste"
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "manage-all",
                    "title": "All Manager",
                    "icon": "media/clipboard.svg"
                }
            ]
        },
        "views": {
            "manage-all": [
                {
                    "id": "managerClipboardHistory",
                    "name": "Clipboard History",
                    "icon": "media/clipboard.png"
                }
            ]
        },
        "commands": [
            {
                "command": "manage-all.editor.copyToHistory",
                "title": "Copy to Clipboard History",
                "category": "All Manager"
            },
            {
                "command": "manage-all.editor.pickAndPaste",
                "title": "Pick and Paste",
                "category": "All Manager"
            },
            {
                "command": "manage-all.editor.showClipboardInFile",
                "title": "Show in the file",
                "category": "All Manager"
            },
            {
                "command": "manage-all.history.clear",
                "title": "Clear History",
                "category": "All Manager",
                "icon": {
                    "dark": "resources/dark/clear-history.svg",
                    "light": "resources/light/clear-history.svg"
                }
            },
            {
                "command": "manage-all.history.remove",
                "title": "Remove",
                "category": "All Manager",
                "icon": {
                    "dark": "resources/dark/remove.svg",
                    "light": "resources/light/remove.svg"
                }
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "manage-all.editor.pickAndPaste",
                    "when": "editorFocus && !editorReadonly"
                },
                {
                    "command": "manage-all.editor.showClipboardInFile",
                    "when": "false"
                },
                {
                    "command": "manage-all.history.clear"
                },
                {
                    "command": "manage-all.history.remove",
                    "when": "false"
                }
            ],
            "view/item/context": [
                {
                    "command": "manage-all.editor.showClipboardInFile",
                    "when": "viewItem =~ /^clipHistoryItem:file/",
                    "group": "0_navigation"
                },
                {
                    "command": "manage-all.history.remove",
                    "when": "viewItem =~ /^clipHistoryItem:/",
                    "group": "1_modification"
                },
                {
                    "command": "manage-all.history.remove",
                    "when": "viewItem =~ /^clipHistoryItem:/",
                    "group": "inline"
                }
            ],
            "view/title": [
                {
                    "command": "manage-all.history.clear",
                    "when": "view == managerClipboardHistory",
                    "group": "navigation"
                }
            ]
        },
        "keybindings": [
            {
                "command": "manage-all.editor.copyToHistory",
                "key": "Ctrl+Shift+C",
                "mac": "Cmd+Shift+C",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "command": "manage-all.editor.pickAndPaste",
                "key": "Ctrl+Shift+V",
                "mac": "Cmd+Shift+V",
                "when": "textInputFocus && !editorReadonly"
            }
        ],
        "configuration": [
            {
                "title": "All Manager",
                "properties": {
                    "manage-all.avoidDuplicates": {
                        "type": "boolean",
                        "default": true,
                        "description": "Avoid duplicate clips in the list"
                    },
                    "manage-all.checkInterval": {
                        "type": "integer",
                        "default": 500,
                        "description": "Time in milliseconds to check changes in clipboard. Set zero to disable."
                    },
                    "manage-all.maxClipboardSize": {
                        "type": "integer",
                        "default": 1000000,
                        "description": "Maximum clipboard size in bytes."
                    },
                    "manage-all.maxClips": {
                        "type": "integer",
                        "default": 100,
                        "description": "Maximum number of clips to save in clipboard"
                    },
                    "manage-all.moveToTop": {
                        "type": "boolean",
                        "default": true,
                        "description": "Move used clip to top in the list"
                    },
                    "manage-all.onlyWindowFocused": {
                        "type": "boolean",
                        "default": true,
                        "description": "Get clips only from VSCode"
                    },
                    "manage-all.preview": {
                        "type": "boolean",
                        "default": true,
                        "description": "View a preview while you are choosing the clip"
                    },
                    "manage-all.saveTo": {
                        "type": [
                            "string",
                            "null",
                            "boolean"
                        ],
                        "default": null,
                        "description": "Set location to save the clipboard file, set false to disable",
                        "scope": "application"
                    },
                    "manage-all.snippet.enabled": {
                        "scope": "resource",
                        "type": "boolean",
                        "default": true,
                        "description": "Enable completion snippets"
                    },
                    "manage-all.snippet.max": {
                        "scope": "resource",
                        "type": "integer",
                        "default": 10,
                        "description": "Maximum number of clips to suggests in snippets (Zero for all)"
                    },
                    "manage-all.snippet.prefix": {
                        "scope": "resource",
                        "type": "string",
                        "default": "clip",
                        "description": "Default prefix for snippets completion (clip1, clip2, ...)"
                    }
                }
            }
        ]
    },
    "scripts": {
        "changelog:last": "conventional-changelog -p angular -r 2",
        "changelog:preview": "conventional-changelog -p angular -u",
        "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "clean": "rimraf ./out ./coverage ./test-reports",
        "compile": "tsc -p ./",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "organize": "node ./out/tools/organize.js",
        "release": "release-it",
        "test": "npm run compile && node ./out/test/runTests.js",
        "vscode:prepublish": "npm run clean && npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/clipboardy": "^2.0.1",
        "@types/glob": "^8.1.0",
        "@types/istanbul-lib-coverage": "^2.0.4",
        "@types/istanbul-lib-hook": "^2.0.1",
        "@types/istanbul-lib-instrument": "^1.7.4",
        "@types/istanbul-lib-report": "^3.0.0",
        "@types/istanbul-lib-source-maps": "^4.0.1",
        "@types/istanbul-reports": "^3.0.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "~18.15.3",
        "@types/sinon": "^10.0.13",
        "@types/vscode": "1.65.0",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "@vscode/test-electron": "^2.3.0",
        "conventional-changelog-cli": "^2.2.2",
        "decache": "^4.6.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "glob": "^9.3.0",
        "istanbul-lib-coverage": "^3.0.0",
        "istanbul-lib-hook": "^3.0.0",
        "istanbul-lib-instrument": "^4.0.3",
        "istanbul-lib-report": "^3.0.0",
        "istanbul-lib-source-maps": "^4.0.1",
        "istanbul-reports": "^3.0.2",
        "mocha": "^10.2.0",
        "mocha-multi-reporters": "^1.5.1",
        "original-fs": "^1.2.0",
        "prettier": "^2.8.4",
        "release-it": "^15.8.0",
        "rimraf": "^4.4.0",
        "sinon": "^15.0.2",
        "source-map-support": "^0.5.21",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "clipboardy": "^2.3.0"
    }
}
